<!DOCTYPE html>
<html>
<head>
    <title>Players - View All</title>
</head>
    <body>
        <h1>Players</h1>

        <table border= "1">
            <tr>
                <th>Player ID</th>
                <th>Jersey Number</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Height (cm)</th>
                <th>Position</th>
                <th>Team ID</th>
            </tr>
            <% for (var i= 0; i < players.length; i++) { %>
                <tr>
                    <td><%= players[i].player_id %></td>
                    <td><%= players[i].jersey_num %></td>
                    <td><%= players[i].first_name%></td>
                    <td><%= players[i].last_name%></td>
                    <td><%= players[i].height%></td>
                    <td><%= players[i].position%></td>
                    <td><%= players[i].team_id%></td>
                 </tr>
             <% } %>
         </table>
    </body>
</html>
