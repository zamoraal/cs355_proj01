<!DOCTYPE html>
<html>
<head>
    <title>Players - View All</title>
</head>
    <body>
        <h1>Players</h1>

        <table border= "1">
            <tr>
                <th>Player ID</th>
                <th>Game ID</th>
                <th>Points</th>
                <th>Assists</th>
                <th>Rebounds</th>
                <th>Steals</th>
                <th>Blocks</th>
                <th>Turnovers</th>
            </tr>
            <% for (var i= 0; i < player_stats.length; i++) { %>
                <tr>
                    <td><%= player_stats[i].player_id %></td>
                    <td><%= player_stats[i].game_id %></td>
                    <td><%= player_stats[i].points%></td>
                    <td><%= player_stats[i].assists%></td>
                    <td><%= player_stats[i].rebounds%></td>
                    <td><%= player_stats[i].steals%></td>
                    <td><%= player_stats[i].blocks%></td>
                    <td><%= player_stats[i].turnovers%></td>
                 </tr>
             <% } %>
         </table>
    </body>
</html>
